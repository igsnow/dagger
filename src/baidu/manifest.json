{
  "manifest_version": 2,
  "name": "自动下单",
  "version": "0.1.0",
  "author": "zhangzhiyong",
  "description": "1688 auto order",
  "icons": {
    "16": "sicon.png",
    "48": "sicon.png",
    "128": "sicon.png"
  },
  "browser_action": {
    "default_icon": "sicon.png",
    "default_title": "action",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "*://localhost/*",
        "*://vwork.vcanbuy.com/*"
      ],
      "js": [
        "content-script.js",
        "jquery-1.11.1.js"
      ]
    }
  ],
  "permissions": [
    "contextMenus"
  ],
  "externally_connectable": {
    "matches": [
      "*://localhost/*",
      "*://vwork.vcanbuy.com/*"
    ]
  }
}
